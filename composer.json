{
    "require": {
        "php": "^8.2",
        "google/apiclient": "^2.12.1",
        "symfony/routing": "^6.2",
        "symfony/http-foundation": "^6.2",
        "symfony/http-kernel": "^6.2",
        "twig/twig": "^3.5",
        "m4tthumphrey/php-gitlab-api": "*",
        "symfony/dotenv": "^6.2",
        "ext-curl": "*",
        "ext-json": "*",
        "rudolfspumpurs/metricize": "^0.1.6"
    },
    "autoload": {
        "psr-4": {
            "RCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "repositories":[
        {
            "type": "vcs",
            "url" : "git@bitbucket.org:rudolfspumpurs/metricize.git"
        }
    ]
}
